# Description:
#   AWS C S3

package(default_visibility = ["//visibility:public"])

licenses(["notice"])  # Apache 2.0

exports_files(["LICENSE"])

cc_library(
    name = "aws_c_s3",
    srcs = glob([
        "include/aws/s3/**/*.h",
        "source/*.c",
    ]),
    includes = ["include"],
    deps = [
        "@com_github_aws_c_auth//:aws_c_auth",
        "@com_github_aws_c_http//:aws_c_http",
        "@com_github_aws_checksums//:aws_checksums",
    ],
)